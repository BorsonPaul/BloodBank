<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#910101"
    android:gravity="center"
    tools:context=".First_page">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"

        android:gravity="center">
        <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginStart="16dp"
            android:onClick="home"
            android:src="@drawable/home"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.023" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:background="#75D9FFFB"
                android:padding="20dp"
                android:gravity="center">

                <LinearLayout
                    android:id="@+id/apos"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:background="#1A237E"
                    android:gravity="center"
                    android:onClick="Apositive"

                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/aplus" />




                </LinearLayout>

                <LinearLayout
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_marginLeft="30dp"
                    android:onClick="ANegative"
                    android:id="@+id/aneg"
                    android:background="#311B92">
                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/amin"
                        />



                </LinearLayout>


            </LinearLayout>
        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:background="#75D9FFFB"
                android:padding="20dp"
                android:gravity="center">

                <LinearLayout
                    android:id="@+id/img_updated"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:background="#1A237E"
                    android:gravity="center"
                    android:onClick="Bpositive"

                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/bpls" />




                </LinearLayout>

                <LinearLayout
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_marginLeft="30dp"
                    android:onClick="BNegative"
                    android:id="@+id/msg_update"
                    android:background="#311B92">
                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/bneg"
                        />



                </LinearLayout>


            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:background="#75D9FFFB"
                android:padding="20dp"
                android:gravity="center">

                <LinearLayout
                    android:id="@+id/abpod"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:background="#1A237E"
                    android:gravity="center"
                    android:onClick="ABpositive"

                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/abpls" />




                </LinearLayout>

                <LinearLayout
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_marginLeft="30dp"
                    android:onClick="ABNegative"
                    android:id="@+id/abneg"
                    android:background="#311B92">
                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/abneg"
                        />



                </LinearLayout>


            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="30dp"
                android:background="#75D9FFFB"
                android:padding="20dp"
                android:gravity="center">

                <LinearLayout
                    android:id="@+id/opos"
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:background="#1A237E"
                    android:gravity="center"
                    android:onClick="Opositive"

                    android:orientation="vertical">

                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/opls" />




                </LinearLayout>

                <LinearLayout
                    android:layout_width="150dp"
                    android:layout_height="150dp"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_marginLeft="30dp"
                    android:onClick="ONegative"
                    android:id="@+id/oneg"
                    android:background="#311B92">
                    <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:src="@drawable/oneg"
                        />



                </LinearLayout>


            </LinearLayout>
        </LinearLayout>
    </LinearLayout>



</androidx.core.widget.NestedScrollView>